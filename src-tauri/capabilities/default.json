{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions and http access",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://music.jiangnan24.com/*"
        },
        {
          "url": "https://music-dl.sayqz.com/*"
        }
      ]
    }
  ]
}